{
  "name": "nuxt-betterauth-cf",
  "version": "1.0.1",
  "description": "Nuxt, Better Auth and Cloudflare ðŸ”¥",
  "repository": "https://github.com/tkjaergaard/nuxt-betterauth-cf",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    },
    "./auth": {
      "import": "./dist/runtime/server/utils/auth.js"
    },
    "./config": {
      "import": "./dist/runtime/config.js"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "pnpm run dev:prepare && nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "pnpm run lint && pnpm run prepack && changelogen --release && pnpm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^4.0.3",
    "better-auth": "^1.3.5",
    "defu": "^6.1.4",
    "drizzle-orm": "^0.44.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250812.0",
    "@nuxt/devtools": "^2.6.2",
    "@nuxt/eslint-config": "^1.8.0",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.0.3",
    "@nuxt/test-utils": "^3.19.2",
    "@types/node": "latest",
    "changelogen": "^0.6.2",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.33.0",
    "nitro-cloudflare-dev": "^0.2.2",
    "nuxt": "^4.0.3",
    "typescript": "~5.9.2",
    "vitest": "^3.2.4",
    "vue-router": "^4.5.1",
    "vue-tsc": "^3.0.5",
    "wrangler": "^4.29.0"
  },
  "peerDependencies": {
    "vue-router": "^4.5.1"
  }
}